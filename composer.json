{
    "name": "memibeltrame/lacekit",
    "license": "MIT",
    "version": "0.0.7",
    "description": "LaceKit is a data driven prototyping framework for designers",
    "type": "project",
    "require": {
        "composer/installers": "^2.0",
        "php": ">=8.2"
    },
    "extra": {
        "installer-paths": {
            "./": ["type:project"]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php vendor/memibeltrame/lacekit/core/postInstall.php"
        ],
        "post-update-cmd": [
            "php vendor/memibeltrame/lacekit/core/postInstall.php"
        ]
    }
}